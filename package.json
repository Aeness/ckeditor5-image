{
  "name": "@aeness/ckeditor5-insert-image",
  "version": "22.0.0",
  "description": "Image feature for CKEditor 5.",
  "dependencies": {
    "@ckeditor/ckeditor5-core": "^23.0.0",
    "@ckeditor/ckeditor5-engine": "^23.0.0",
    "@ckeditor/ckeditor5-image": "^23.0.0",
    "@ckeditor/ckeditor5-ui": "^23.0.0",
    "@ckeditor/ckeditor5-utils": "^23.0.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^23.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^23.0.0",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^23.0.0",
    "@ckeditor/ckeditor5-editor-classic": "^23.0.0",
    "@ckeditor/ckeditor5-essentials": "^23.0.0",
    "@ckeditor/ckeditor5-link": "^23.0.0",
    "@ckeditor/ckeditor5-paragraph": "^23.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^23.0.0",
    "eslint": "^7.16.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.7.1"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aeness/ckeditor5-image.git"
  },
  "files": [
    "lang",
    "src",
    "theme"
  ],
  "scripts": {
    "watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "lint": "eslint --quiet .",
    "stylelint": "stylelint --quiet --allow-empty-input 'theme/**/*.css' 'docs/**/*.css'"
  },
  "stylelint": {
    "extends": "stylelint-config-recommended",
    "rules": {
      "at-rule-no-unknown": null,
      "declaration-property-value-blacklist": {
        "/.*/": [
          "/#/",
          "/rgb/"
        ]
      },
      "color-named": "never"
    }
  }
}
